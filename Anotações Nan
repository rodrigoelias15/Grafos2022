void bubbleSort(int vet[], int n){

comp = 0;
mov = 0;

for(int i=0; i<n-1; i++)
{
    for(int j=0; j<n-1; j++)
    {
        if(vet[j]>vet[j+1])
            comp++;

            int auxiliar[] = vet[j];

            mov++;
            
    }
    cout << "Quantidade de comparacoes:  " comp  << endl;
    cout << "Quantidade movimentadas:  " mov  << endl;

}
retur auxiliar[];

}


